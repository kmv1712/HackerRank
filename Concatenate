import numpy

n, m, p = input().split(' ')

n_array = [input().split(" ") for i in range (int(n)) ]
m_array = [input().split(" ") for i in range (int(m)) ]

n_array = numpy.array(n_array, int)
m_array = numpy.array(m_array, int)

print (numpy.concatenate((n_array, m_array), axis = 0))   
